{
  "name": "husky-init",
  "version": "8.0.0",
  "description": "Install and setup husky quickly",
  "keywords": [
    "husky",
    "init",
    "git",
    "hooks",
    "pre-commit"
  ],
  "homepage": "https://typicode.github.io/husky",
  "repository": "typicode/husky-init",
  "funding": "https://github.com/sponsors/typicode",
  "license": "MIT",
  "author": "Typicode <typicode@gmail.com>",
  "main": "lib/index.js",
  "bin": "lib/bin.js",
  "files": [
    "lib",
    "!lib/**/*.test.js"
  ],
  "scripts": {
    "build": "tsc",
    "test": "npm run build && xv lib",
    "lint": "eslint . --ext .ts --ignore-path .gitignore",
    "prepare": "husky install"
  },
  "dependencies": {
    "husky": "^8.0.0"
  },
  "devDependencies": {
    "@tsconfig/node12": "^1.0.9",
    "@types/node": "^17.0.31",
    "@typicode/eslint-config": "^1.0.0",
    "type-fest": "^2.12.2",
    "typescript": "^4.6.4",
    "xv": "^1.1.1"
  },
  "engines": {
    "node": ">=14"
  }
}
